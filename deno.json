{
  "name": "@tettuan/breakdownlogger",
  "version": "1.0.3",
  "exports": {
    ".": "./mod.ts",
    "./logger": "./src/logger.ts"
  },
  "description": "A debug logging library for Deno applications",
  "repository": "https://github.com/tettuan/breakdownlogger",
  "keywords": [
    "deno",
    "logging",
    "debug",
    "test",
    "development"
  ],
  "author": "tettuan",
  "license": "MIT",
  "imports": {
    "std/": "https://deno.land/std@0.220.1/"
  },
  "tasks": {
    "test": "deno test --allow-env",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check **/*.ts",
    "publish": "deno publish",
    "ci": "bash scripts/local_ci.sh"
  },
  "publish": {
    "include": [
      "mod.ts",
      "src/**/*.ts",
      "README.md"
    ]
  }
}
